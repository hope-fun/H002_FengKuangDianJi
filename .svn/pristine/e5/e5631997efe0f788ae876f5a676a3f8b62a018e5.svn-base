<script>

    $('.lanrenzhijia').height(window.innerHeight - 60);
    function onDownloadClick(nFlag) {
        if (1 == nFlag) {
            if (browser.isAndroid) {
                window.location.href = "https://play.google.com/store/apps/details?id=com.snsplus.heroesstrikehd";
            }
            if (browser.isIos) {
                window.location.href = "https://itunes.apple.com/th/app/heroes-strike/id973863543";
            }
        }
        else if (2 == nFlag) {
            if (browser.isAndroid) {
                window.location.href = "http://shouji.baidu.com/game/item?docid=3880046&from=landing&f=search_app_%E7%8E%A9%E8%BD%AC%E5%8D%97%E4%BA%AC%40list_1_title%401%40header_game_input";
            }
            if (browser.isIos) {
                window.location.href = "https://itunes.apple.com/cn/app/wan-zhuan-nan-jing/id641003103?mt=8";
            }
        }
        else if (3 == nFlag) {
            if (browser.isAndroid) {
                window.location.href = "http://shouji.baidu.com/game/item?docid=5706041&from=landing&f=search_app_%E8%90%8C%E6%96%B9%E8%A8%80%40list_1_title%401%40header_game_input";
            }
            if (browser.isIos) {
                window.location.href = "https://itunes.apple.com/cn/app/meng-fang-yan/id738956253?mt=8";
            }
        }

        //alert($('#buttom').width());
    }
    function mouseover(nFlag, obj) {
        if (1 == nFlag) {
            var imgobj = document.getElementById("zhuye");
            imgobj.src = "img/zhuye_select.png";
        }
        else if (2 == nFlag) {
            var imgobj = document.getElementById("youxi");
            imgobj.src = "img/youxi_select.png";
        }
        else if (3 == nFlag) {
            var imgobj = document.getElementById("huodong");
            imgobj.src = "img/huodong_select.png";
        }
        else if (4 == nFlag) {
            var imgobj = document.getElementById("wode");
            imgobj.src = "img/wode_select.png";
        }

        obj.style.color = "#ff9968";
    }
    function mouseout(nFlag, obj) {
        if (1 == nFlag) {
            var imgobj = document.getElementById("zhuye");
            imgobj.src = "img/zhuye.png";
        }
        else if (2 == nFlag) {
            var imgobj = document.getElementById("youxi");
            imgobj.src = "img/youxi.png";
        }
        else if (3 == nFlag) {
            var imgobj = document.getElementById("huodong");
            imgobj.src = "img/huodong.png";
        }
        else if (4 == nFlag) {
            var imgobj = document.getElementById("wode");
            imgobj.src = "img/wode.png";
        }

        obj.style.color = "";
    }
</script>

